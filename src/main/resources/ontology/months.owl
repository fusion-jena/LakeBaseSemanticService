@prefix : <http://fusion.cs.uni-jena.de/ontologies/lakebase/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix time: <http://www.w3.org/2006/time#> .

<http://fusion.cs.uni-jena.de/ontologies/lakebase/months>
	rdf:type owl:Ontology ;
	rdfs:label "Months" ;
	dcterms:creator <http://orcid.org/0000-0002-7733-0193> ;
	dcterms:license <http://creativecommons.org/licenses/by/4.0/> .

:January
	rdf:type owl:Class ;
	rdfs:subClassOf time:DateTimeDescription ,
		[	rdf:type owl:Restriction ;
			owl:onProperty time:unitType ;
			owl:hasValue time:unitMonth
		] ,
		[	rdf:type owl:Restriction ;
			owl:onProperty time:month ;
			owl:hasValue 1
		] ;
	rdfs:label "Januar"@de ,
		"January"@en .

