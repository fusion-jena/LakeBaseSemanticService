@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix om: <http://www.ontology-of-units-of-measure.org/resource/om-2/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix time: <http://www.w3.org/2006/time#> .

<http://fusion.cs.uni-jena.de/ontologies/lakebase/mappingTimeAndOM2>
	rdf:type owl:Ontology ;
	rdfs:label "Mapping of OWL-Time and OM 2" ;
	dcterms:creator <http://orcid.org/0000-0002-7733-0193> ;
	dcterms:license <http://creativecommons.org/licenses/by/4.0/> .

time:unitDay	owl:sameAs om:day .
time:unitHour	owl:sameAs om:hour .
time:unitMinute	owl:sameAs om:minute-Time .
time:unitMonth	owl:sameAs om:month .
time:unitSecond	owl:sameAs om:second-Time .
time:unitWeek	owl:sameAs om:week .
time:unitYear	owl:sameAs om:year .
